{% extends 'AppBundle::base.html.twig' %}

{% block body -%}
    <h1>Emisor</h1>

    <table class="record_properties table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Razonsocial</th>
                <td>{{ entity.razonSocial }}</td>
            </tr>
            <tr>
                <th>Cuit</th>
                <td>{{ entity.cuit }}</td>
            </tr>
            <tr>
                <th>Domicilio</th>
                <td>{{ entity.domicilio }}</td>
            </tr>
            <tr>
                <th>Condicioniva</th>
                <td>{{ entity.condicionIva }}</td>
            </tr>
            <tr>
                <th>Fechainicioactividades</th>
                <td>{{ entity.fechaInicioActividades|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('emisor') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('emisor_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
